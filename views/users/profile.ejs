<%- include('../partials/header') %>

<section class="wrapper">
  <h2 class="card-title">
    <%= user.name %>
  </h2>
  <a href="/users/<%= user._id %>/edit" class="profile">Edit Profile</a>
  <a href="/matches" class="profile">View Matches</a>
  <a href="/signs" class="profile">Review Signs</a>
  <hr>
  <p class="card-text">Email: <%= user.email %>
  </p>
  <p class="card-text">Status: <%= user.status %>
  </p>
  <p class="card-text">Sun Sign: <%= user.sunSign %>
  </p>
  <p class="card-text">Moon Sign: <%= user.moonSign %>
  </p>
  <p class="card-text">Asc Sign: <%= user.ascSign %>
  </p>
</section>

<%- include('../partials/footer') %>