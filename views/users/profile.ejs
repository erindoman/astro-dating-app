<%- include('../partials/header') %>


  <div class="card">
    <div class="tab-content">
      <div class="tab-pane fade show active">
        <div class="card-body">
          <h3 class="card-title">
            Welcome back, <%= user.name %>! 
          </h3>
          <a href="/user/<%= user._id %>?_method=PUT">Edit Profile</a>
          <a href="/matches">View Matches</a>
          <p class="card-text">Email: <%= user.email %>
          </p>
          <p class="card-text">Bio: <%= user.bio %>
          </p>
          <p class="card-text">Sun sign: [placeholder]</p>
          <p class="card-text">Moon sign: [placeholder]</p>
          <p class="card-text">Asc sign: [placeholder]</p>
        </div>
      </div>
    </div>
  </div>

  <%- include('../partials/footer') %>